  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Build via yarn",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["build"],
      "port": 9229,
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Test via yarn",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["test"],
      "port": 9229,
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch via yarn",
      "cwd": "${workspaceFolder}/doc",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["start"],
      "port": 9229,
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
